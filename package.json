    "dependencies": {
        "ajv": "^8.12.0",
        "axios": "^1.5.0",
        "express": "^4.18.2",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
