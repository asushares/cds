{
    "name": "@asushares/cds",
    "version": "3.0.0",
    "type": "module",
    "description": "ASU SHARES FHIR Consent CDS Hooks services.",
    "scripts": {
        "start": "npm run build && node build/server.js",
        "build": "npx tsc && npm run copy-files",
        "copy-files": "cp -Rf src/data/ build/data/",
        "watch": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'npx tsx src/server.ts'",
        "test": "node --test test/**/*.test.ts",
        "test-watch": "nodemon --watch '{src,test}/**' --ext 'ts,json' --exec 'npx tsx --test test/**/*.test.ts'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/asushares/cds.git"
    },
    "keywords": [
        "asu shares",
        "cds hooks"
    ],
    "author": "Preston Lee",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/asushares/cds/issues"
    },
    "homepage": "https://github.com/asushares/cds#readme",
    "dependencies": {
        "@asushares/core": "^3.1.1",
        "ajv": "^8.17.1",
        "axios": "^1.12.2",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-basic-auth": "^1.2.1",
        "jsonpath-plus": "^10.3.0",
        "nodemon": "^3.1.10",
        "rxjs": "^7.8.2",
        "statman": "^1.9.1",
        "supertest": "^7.1.4",
        "typescript": "^5.9",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/fhir": "^0.0.41",
        "@types/node": "^24.8.1",
        "@types/uuid": "^11.0.0",
        "tsx": "^4.20.6"
    }
}
